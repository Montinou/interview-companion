# Clerk Authentication
# These are placeholder keys for local builds.
# Set the real values in Vercel environment variables.
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_placeholder_key_for_build_only
CLERK_SECRET_KEY=sk_test_placeholder_key_for_build_only
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/dashboard

# Database (NeonDB)
# For local builds, use a placeholder. Set the real value in Vercel.
DATABASE_URL=postgresql://placeholder:placeholder@placeholder.neon.tech/placeholder

# OpenAI (for AI features - optional)
OPENAI_API_KEY=

# Pusher (for real-time - optional)
NEXT_PUBLIC_PUSHER_KEY=
NEXT_PUBLIC_PUSHER_CLUSTER=
PUSHER_APP_ID=
PUSHER_SECRET=
